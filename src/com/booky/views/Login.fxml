      <TextField fx:id="passwordField" layoutX="181.0" layoutY="200.0" prefHeight="26.0" prefWidth="238.0" />
      <Label layoutX="103.0" layoutY="205.0" text="Password" />
      <Button fx:id="loginBtn" layoutX="252.0" layoutY="263.0" mnemonicParsing="false" onAction="#loginButtonClick" prefHeight="45.0" prefWidth="95.0" text="Login" />
      <Button fx:id="registerBtn" layoutX="356.0" layoutY="263.0" mnemonicParsing="false" onAction="#registerBtnClick" prefHeight="45.0" prefWidth="95.0" text="Register" />
